<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.campos0022715.labosindustrial">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="userPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlumnosMain"
            android:label="@string/title_activity_alumnos_main"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InscripcionLabos"
            android:label="@string/title_activity_inscripcion_labos"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MisLabos"
            android:label="@string/title_activity_mis_labos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetallesMLabos"
            android:label="@string/title_activity_detalles_mlabos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AnuncioMain"
            android:label="@string/title_activity_anuncio_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AnuncioInfo"
            android:label="@string/title_activity_anuncio_info"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Informacion"
            android:label="@string/title_activity_informacion"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>